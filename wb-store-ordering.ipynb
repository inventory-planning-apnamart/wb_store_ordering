        "def cal_mbq(multiplier, mbq, cat):\n",
        "  if cat == 'mt_ad_hoc' or cat == 'visibility' or cat == 'loose':\n",
        "    return mbq\n",
        "  elif cat == 'gift':\n",
        "    return round(multiplier*mbq,1)\n",
        "  else:\n",
        "    return mround((multiplier*mbq),2)"
      ]
