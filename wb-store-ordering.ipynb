        "        except gspread.exceptions.APIError as e:\n",
        "            if e.response and e.response.status_code == 429:\n",
        "                wait = 2 ** i + random.uniform(0, 1)\n",
        "                print(f\"⚠️  Quota exceeded. Retrying in {wait:.2f} seconds...\")\n",
        "                time.sleep(wait)\n",
